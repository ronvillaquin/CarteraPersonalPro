        android:theme="@style/AppTheme">
        <activity
            android:name="com.rrvq.carterapersonalpro.Bienvenida"
            android:screenOrientation="portrait"/>

        <activity android:name="com.rrvq.carterapersonalpro.SplashScream"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.rrvq.carterapersonalpro.Main2Activity_Transferencia_edit"
