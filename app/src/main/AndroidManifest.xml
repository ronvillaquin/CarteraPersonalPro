            android:parentActivityName="com.rrvq.carterapersonalpro.Main2Activity_Categorias"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.rrvq.carterapersonalpro.Main2Activity_Cuentas"
            android:parentActivityName="com.rrvq.carterapersonalpro.MainActivity"
